[sqlfluff]
templater = jinja
dialect = snowflake

[sqlfluff:indentation]
indented_joins = True

# Some rules can be configured directly from the config common to other rules.
[sqlfluff:rules]
tab_space_size = 4
max_line_length = 100
indent_unit = space

[sqlfluff:layout:type:comma]
line_position = leading